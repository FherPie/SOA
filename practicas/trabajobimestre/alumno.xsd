<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.alumno.com"
    elementFormDefault="qualified"
    xmlns="http://www.alumno.com"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:per="http://personas.com"
    xmlns:doc="http://docencia.com">
  <xs:import schemaLocation="personas.xsd" namespace="http://personas.com" />
  <xs:import schemaLocation="docencia.xsd" namespace="http://docencia.com" />
  <xs:element name="alumno">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="per:persona"/>
        <xs:element ref="doc:datos_academicos"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  
</xs:schema>
